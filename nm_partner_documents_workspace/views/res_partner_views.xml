<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>

		<record id="inherit_customer_form" model="ir.ui.view">
			<field name="name">inherit.customer.form</field>
			<field name="model">res.partner</field>
			<field name="inherit_id" ref="nm_reception_treatment_unit.customer_form_view" />
			<field name="arch" type="xml">
				<xpath expr="//button[@name='view_acupuncture_form']" position="after">
					<button name="action_open_documents" class="oe_stat_button" icon="fa-file-text" type="object">
						<field name="document_count" widget="statinfo" string="Documents" modifiers="{&quot;readonly&quot;: true}"/>
					</button>
				</xpath>
			</field>
		</record>
	</data>
</odoo>